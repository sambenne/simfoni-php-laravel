{
  "name": "mblsolutions/simfoni-php-laravel",
  "description": "Official Simfoni interface for Laravel PHP applications.",
  "keywords": ["simfoni", "giftcard", "e-gift", "laravel"],
  "type": "package",
  "license": "MIT",
  "authors": [
    {
      "name": "Redu Retail",
      "email": "tech@redu.co.uk"
    }
  ],
  "require": {
    "ext-json": "*",
    "illuminate/events": "~5.6.0|~5.7.0|~5.8.0|^6.0|^7.0|^8.0|^9.0|^10.0|^11.0",
    "illuminate/routing": "~5.6.0|~5.7.0|~5.8.0|^6.0|^7.0|^8.0|^9.0|^10.0|^11.0",
    "illuminate/support": "~5.6.0|~5.7.0|~5.8.0|^6.0|^7.0|^8.0|^9.0|^10.0|^11.0",
    "illuminate/session": "~5.6.0|~5.7.0|~5.8.0|^6.0|^7.0|^8.0|^9.0|^10.0|^11.0",
    "illuminate/pagination": "~5.6.0|~5.7.0|~5.8.0|^6.0|^7.0|^8.0|^9.0|^10.0|^11.0",
    "mblsolutions/simfoni-php": "^3.0"
  },
  "require-dev": {
    "orchestra/testbench": "^8.0",
    "phpunit/phpunit": "^8.0|^9.3"
  },
  "autoload": {
    "psr-4": {
      "MBLSolutions\\SimfoniLaravel\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "MBLSolutions\\SimfoniLaravel\\Tests\\": "tests"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "extra": {
    "laravel": {
      "providers": [
        "MBLSolutions\\SimfoniLaravel\\SimfoniServiceProvider"
      ]
    }
  }
}
